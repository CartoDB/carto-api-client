{
  "name": "@carto/api-client",
  "private": true,
  "packageManager": "yarn@4.2.2",
  "author": "Don McCurdy <donmccurdy@carto.com>",
  "license": "UNLICENSED",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/api-client.cjs",
  "module": "./dist/api-client.modern.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/api-client.cjs",
      "default": "./dist/api-client.modern.js"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "node >= 18"
  ],
  "scripts": {
    "build": "microbundle --format cjs,modern --no-compress --define VERSION=$npm_package_version",
    "build:watch": "microbundle watch --format cjs,modern --no-compress --define VERSION=$npm_package_version",
    "dev": "concurrently \"yarn build:watch\" \"vite --config examples/vite.config.ts --open\"",
    "test": "vitest --typecheck",
    "coverage": "vitest run --coverage",
    "clean": "rimraf dist/*",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write"
  },
  "devDependencies": {
    "@deck.gl/aggregation-layers": "^9.0.14",
    "@deck.gl/carto": "^9.0.14",
    "@deck.gl/core": "^9.0.14",
    "@deck.gl/extensions": "^9.0.14",
    "@deck.gl/geo-layers": "^9.0.14",
    "@deck.gl/layers": "^9.0.12",
    "@deck.gl/mesh-layers": "^9.0.12",
    "@deck.gl/react": "^9.0.17",
    "@lit/react": "^1.0.5",
    "@lit/task": "^1.0.1",
    "@loaders.gl/core": "^4.2.1",
    "@luma.gl/core": "^9.0.12",
    "@luma.gl/engine": "^9.0.12",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "^8.2.2",
    "echarts": "^5.5.0",
    "lit": "^3.1.4",
    "lit-analyzer": "^1.2.1",
    "maplibre-gl": "^4.1.3",
    "microbundle": "^0.15.1",
    "prettier": "^2.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "^7.1.7",
    "rimraf": "^3.0.2",
    "svelte": "^4.2.17",
    "typescript": "~5.3.3",
    "vite": "^5.2.10",
    "vitest": "1.6.0",
    "vue": "^3.4.27"
  }
}
